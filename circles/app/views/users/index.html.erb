<h1>用户列表</h1>

<div class="row">
  <div class="col-sm-12">
    <table class="table">
      <tr>
        <th>ID</th>
        <th>用户名</th>
        <th></th>
      </tr>
      <% @users.each do |user| %>
        <tr>
          <th><%= user.id%></th>
          <th><%=user.username%></th>
          <th></th>
        </tr>
        <% end -%>
    </table>
    <%= will_paginate @users %>
  </div>
</div>